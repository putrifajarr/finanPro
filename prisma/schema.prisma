generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id                 Int       @id @default(autoincrement())
  nama_lengkap       String
  email              String    @unique
  password           String?
  googleid           String?   @unique
  tanggal_registrasi DateTime? @default(now()) @db.Timestamp(6)
}

